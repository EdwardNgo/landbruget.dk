# BMD Portal Configuration
BMD_BASE_URL=https://bmd.mst.dk

# Directory Settings
BRONZE_OUTPUT_DIR=/data/bronze/bmd
SILVER_OUTPUT_DIR=/data/silver/bmd

# Processing Settings
OVERWRITE_EXISTING=false
LOG_LEVEL=INFO

# Deployment Settings
# Set to 'development' for local development, 'container' for Docker, 'production' for production
ENVIRONMENT=development

# GCS Settings (used only in production environment)
GCS_BUCKET_NAME=your-gcs-bucket-name
